@{
	ViewData["Title"] = "Cars Page";
}
@model CatModel

<!-- Start Hero Section -->
<div class="hero">
	<div class="container">
		<div class="row justify-content-between">
			<div class="col-lg-5">
				<div class="intro-excerpt">
					<h1>@Model.CatName</h1>
				</div>
			</div>
			<div class="col-lg-7">
			</div>
		</div>
	</div>
</div>
<!-- End Hero Section -->

<div class="untree_co-section product-section before-footer-section">
	<div class="container">
		<div class="row">
			@await Html.PartialAsync("_ItemPartial", Model)
		</div>
	</div>
</div>
